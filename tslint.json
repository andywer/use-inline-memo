{
  "extends": [
    "tslint:latest",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": [
      true,
      "never-prefix"
    ],
    "no-implicit-dependencies": [
      true,
      "dev"
    ],
    "no-submodule-imports": false,
    "object-literal-sort-keys": false,
    "only-arrow-functions": false,
    "semicolon": false
  },
  "rulesDirectory": []
}
